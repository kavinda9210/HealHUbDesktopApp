{
  "expo": {
    "name": "HealHubMobileApp",
    "slug": "HealHubMobileApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/HealHub_icon.png",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "HealHub needs your location to support ambulance dispatch.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "HealHub needs your location in the background to share ambulance position when you enable location sharing."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/HealHub_icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.kavinda1234.HealHubMobileApp",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "FOREGROUND_SERVICE",
        "FOREGROUND_SERVICE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-splash-screen",
      "@react-native-community/datetimepicker",
      "expo-notifications",
      "expo-secure-store"
    ],
    "extra": {
      "eas": {
        "projectId": "71c7e625-a3d5-4c7f-84dd-2cad3fcfc76e"
      }
    }
  }
}
